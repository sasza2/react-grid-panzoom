{
  "name": "react-grid-panzoom",
  "version": "1.4.1",
  "description": "React grid component for pan and zoom with possibility to moving and selecting elements inside",
  "keywords": [
    "react",
    "grid",
    "pan",
    "zoom",
    "move",
    "panzoom",
    "moving"
  ],
  "main": "dist/main.umd.js",
  "module": "./dist/main.mjs",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.umd.js"
    }
  },
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc && vite build",
    "changeset": "changeset",
    "dev": "ladle serve",
    "lint": "eslint src --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepack": "npm run build",
    "publish:package": "changeset publish",
    "version:package": "changeset version"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sasza2/react-grid-panzoom.git"
  },
  "author": "sasza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sasza2/react-grid-panzoom/issues"
  },
  "browser": {
    "[module-name]": false
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@ladle/react": "4.0.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.2",
    "@testing-library/react-hooks": "8.0.1",
    "@types/react": "18.0.26",
    "@types/react-color": "3.0.6",
    "@types/react-toggle": "4.0.3",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "@vitest/coverage-v8": "0.34.6",
    "changeset": "0.2.6",
    "eslint": "8.40.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "22.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4",
    "vite": "5.0.12",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@sasza/react-panzoom": "1.13.0"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  }
}
